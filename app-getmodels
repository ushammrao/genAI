from openai import OpenAI
from dotenv import load_dotenv
import os

load_dotenv()

client = OpenAI(
    base_url = "https://integrate.api.nvidia.com/v1",
     api_key = "NVIDIA_API_KEY"
)

models = client.models.list()

for model in models.data:
    print(model.id)

